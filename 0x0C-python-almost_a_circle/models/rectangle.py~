#!/usr/bin/python3
"""module of class rectangle"""
from models.base import Base


class Rectangle(Base):
    """Class Rectangle""" 
    def __init__(self, width, height, x=0, y=0, id=None): 
        """Init function"""
        super().__init__(id)
        self.width = width
        self.height = height
        self.x = x
        self.y = y
